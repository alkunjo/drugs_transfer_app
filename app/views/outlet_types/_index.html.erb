<% @outlet_types.each do |outlet_type| %>
  <tr>
  	<td><%= outlet_type.otype_name %></td>
  	<td>
    	<%= link_to edit_outlet_type_path(outlet_type), class: "btn btn-primary btn-sm", remote: true, html:{style: "color:#FFFFFF"} do %><span class="fa fa-edit"></span> Ubah<% end %>
    	<%= link_to del_outlet_type_path(outlet_type), class: "btn btn-danger btn-sm", remote: true, html:{style: "color:#FFFFFF"} do %><span class="fa fa-trash"></span> Hapus<% end %> </td>
  </tr>
<% end %>