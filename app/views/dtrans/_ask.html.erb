<div style="max-width:600px; margin-left:10px" id="dta_index">
  <% flash.each do |key, value| %>
    <div class="alert alert-<%= key %>">
      <button class="close" data-dismiss="alert" aria-label="close">&times;</button>
      <%= value %>
    </div>
  <% end %>

  <div id="dta_form"></div>
  <% if @transaksi.dtrans.present? %>
  <table class="table table-striped table-bordered">
    <thead>
      <tr>
        <th>Obat</th>
        <th>Permintaan</th>
        <% if @transaksi.unvalidated? %>
        <th colspan="3">Aksi</th>
        <% end %>
      </tr>
    </thead>

    <tbody id="dta_tbl">
      <%= render partial: 'dtrans/ask2' %>
    </tbody>
  </table>
  <% end %>
  
</div>