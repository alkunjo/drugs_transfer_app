<% @satuans.each do |satuan| %>
  <tr>
  	<td><%= satuan.satuan_code %></td>
  	<td><%= satuan.satuan_ukuran %></td>
  	<td><%= satuan.satuan_isi %></td>
  	<td>
    	<%= link_to edit_satuan_path(satuan), class: "btn btn-primary btn-sm", remote: true, html:{style: "color:#FFFFFF"} do %><span class="fa fa-edit"></span> Ubah<% end %>
    	<%= link_to del_satuan_path(satuan), class: "btn btn-danger btn-sm", remote: true, html:{style: "color:#FFFFFF"} do %><span class="fa fa-trash"></span> Hapus<% end %> </td>
  </tr>
<% end %>